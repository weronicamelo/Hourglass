{
  "manifest_version": 3,
  "name": "Hourglass",
  "description": "Extensão de Cronômetro e Timer",
  "version": "1.0",
  "action": {
    "default_popup": "index.html",
    "default_title": "Hourglass"
  },
  "icons": {
    "16": "icons/logo16.png",
    "48": "icons/logo48.png",
    "128": "icons/logo128.png",
    "300": "icons/logo300.png"
  },
  "permissions": ["storage"]
}
